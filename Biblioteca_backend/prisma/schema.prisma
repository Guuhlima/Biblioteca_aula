datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           String   @id @default(cuid())
  email        String   @unique
  nome         String?
  passwordHash String
  createdAt    DateTime @default(now())

  @@map("user")
}

